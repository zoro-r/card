#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jiangdaofeng/Desktop/基础项目/card/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@24.0.12_typescript@5.8.3/node_modules/ts-node-dev/lib/node_modules:/Users/jiangdaofeng/Desktop/基础项目/card/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@24.0.12_typescript@5.8.3/node_modules/ts-node-dev/node_modules:/Users/jiangdaofeng/Desktop/基础项目/card/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@24.0.12_typescript@5.8.3/node_modules:/Users/jiangdaofeng/Desktop/基础项目/card/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jiangdaofeng/Desktop/基础项目/card/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@24.0.12_typescript@5.8.3/node_modules/ts-node-dev/lib/node_modules:/Users/jiangdaofeng/Desktop/基础项目/card/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@24.0.12_typescript@5.8.3/node_modules/ts-node-dev/node_modules:/Users/jiangdaofeng/Desktop/基础项目/card/node_modules/.pnpm/ts-node-dev@2.0.0_@types+node@24.0.12_typescript@5.8.3/node_modules:/Users/jiangdaofeng/Desktop/基础项目/card/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node-dev/lib/bin.js" "$@"
else
  exec node  "$basedir/../ts-node-dev/lib/bin.js" "$@"
fi
